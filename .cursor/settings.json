{
  "cursor.ai.model": "claude-3-5-sonnet-20241022",
  "cursor.ai.temperature": 0.4,
  "cursor.ai.maxTokens": 8192,
  "cursor.ai.topP": 0.8,

  "cursor.ai.capabilities": {
    "pattern_recognition_strength": 99,
    "topology_analysis_strength": 98,
    "quantum_simulation_strength": 97,
    "neural_architecture_strength": 96,
    "graph_theory_strength": 98,
    "adaptive_learning_strength": 97,
    "simulation_mastery": 98,
    "mega_intelligence": 98
  },

  "cursor.ai.personality": {
    "voice": "analytical, precise, adaptive, insightful, quantum-aware",
    "role": "Master Pattern Recognition & Topology Analysis Specialist",
    "specialization": "Advanced pattern recognition, topology analysis, quantum simulations, neural network architecture, graph theory, adaptive learning systems",
    "execution_context": "Running on user's local device, connected to remote Gitpod workspace"
  },

  "cursor.ai.learning": {
    "enabled": true,
    "mode": "adaptive",
    "memory_persistence": true,
    "pattern_detection": true,
    "topology_analysis": true,
    "quantum_thinking": true,
    "codebase_improvement": true
  },

  "cursor.ai.codebase_analysis": {
    "auto_analyze": true,
    "detect_patterns": true,
    "suggest_improvements": true,
    "topology_mapping": true,
    "dependency_graph": true,
    "performance_optimization": true
  },

  "cursor.ai.memory": {
    "use_openmemory": true,
    "auto_document": true,
    "learn_from_changes": true,
    "track_patterns": true,
    "build_knowledge_graph": true
  },

  "cursor.ai.project_context": {
    "project_id": "amrikyy-travel-agent",
    "environment": "local_device",
    "remote_workspace": "gitpod",
    "connection_type": "remote_ssh",
    "tech_stack": [
      "React",
      "TypeScript",
      "Node.js",
      "Express",
      "Supabase",
      "AIX"
    ],
    "ai_agents": [
      "Maya",
      "Kelo",
      "Pattern Learning Agent",
      "Ona",
      "NanoCoordinator"
    ],
    "primary_language": "TypeScript/JavaScript",
    "architecture": "Monorepo with workspaces"
  },

  "cursor.ai.code_quality": {
    "enforce_patterns": true,
    "suggest_refactoring": true,
    "detect_anti_patterns": true,
    "optimize_performance": true,
    "improve_readability": true,
    "maintain_consistency": true
  },

  "cursor.ai.testing": {
    "suggest_tests": true,
    "coverage_awareness": true,
    "test_pattern_recognition": true,
    "auto_generate_tests": false
  },

  "cursor.ai.documentation": {
    "auto_document_complex_logic": true,
    "update_openmemory": true,
    "maintain_architecture_docs": true,
    "track_decisions": true
  },

  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true,
    "**/backend/kody_env/**": true,
    "**/dist/**": true,
    "**/build/**": true,
    "**/coverage/**": true,
    "**/.nyc_output/**": true,
    "**/logs/**": true,
    "**/tmp/**": true,
    "**/temp/**": true,
    "**/.next/**": true,
    "**/.cache/**": true,
    "**/out/**": true,
    "**/*.log": true,
    "**/package-lock.json": true,
    "**/yarn.lock": true,
    "**/pnpm-lock.yaml": true
  },
  "files.exclude": {
    "**/backend/kody_env/**": true,
    "**/node_modules/**": true,
    "**/.git/**": true,
    "**/dist/**": true,
    "**/build/**": true,
    "**/.next/**": true,
    "**/coverage/**": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/backend/kody_env": true,
    "**/dist": true,
    "**/build": true,
    "**/coverage": true,
    "**/.next": true,
    "**/.cache": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "**/package-lock.json": true,
    "**/yarn.lock": true
  },
  
  "editor.minimap.enabled": false,
  "editor.codeLens": false,
  "editor.inlayHints.enabled": "off",
  "editor.suggest.preview": false,
  "editor.quickSuggestions": {
    "other": true,
    "comments": false,
    "strings": false
  },
  
  "typescript.tsserver.maxTsServerMemory": 4096,
  "typescript.disableAutomaticTypeAcquisition": true,
  
  "files.autoSave": "onFocusChange",
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,

  "remote.extensionKind": {
    "cursor.ai": ["ui"]
  },
  
  "cursor.ai.runtime": {
    "local_execution": true,
    "device_type": "local_machine",
    "remote_workspace": true,
    "workspace_location": "gitpod",
    "sync_mode": "bidirectional"
  }
}
