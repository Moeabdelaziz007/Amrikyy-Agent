    "prom-client": "^15.1.3",
    "redis": "^5.8.3",
    "stripe": "^13.6.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/supertest": "^6.0.3",
    "chalk": "^5.6.2",
    "eslint": "^8.50.0",
    "jest": "^30.2.0",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4"
  },
  "keywords": [
    "ai",
