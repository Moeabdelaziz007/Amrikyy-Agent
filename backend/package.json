{
  "name": "amrikyy-backend",
  "version": "1.0.0",
  "description": "Amrikyy AI Automation Platform Backend API Server",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon server.js",
    "start": "node server.js",
    "build": "echo 'Backend build completed'",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "jest src/tests/unit --coverage --passWithNoTests",
    "test:integration": "node src/tests/integration/complete-system-simulation.test.js",
    "test:api": "jest src/tests/api --passWithNoTests",
    "test:security": "npm audit --audit-level=high",
    "smoke-test": "node smoke-tests/smoke-test.js",
    "test:simulation": "node test-simulation.js"
  },
  "dependencies": {
    "@confluentinc/kafka-javascript": "^1.6.0",
    "@grpc/grpc-js": "^1.14.0",
    "@grpc/proto-loader": "^0.8.0",
    "@sentry/node": "^10.19.0",
    "@supabase/supabase-js": "^2.74.0",
    "amadeus": "^11.0.0",
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "connect-redis": "^9.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.0",
    "express-rate-limit": "^6.7.0",
    "express-session": "^1.18.2",
    "helmet": "^6.0.0",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.0.0",
    "multer": "^2.0.2",
