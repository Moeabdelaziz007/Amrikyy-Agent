# Maya Travel Agent dbt Environment Variables
# Copy this file to .env and fill in your Supabase PostgreSQL connection details

# Supabase PostgreSQL Connection (Development)
DB_HOST=your_supabase_host_here
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=your_supabase_password_here
DB_NAME=postgres

# Supabase PostgreSQL Connection (Production)
DB_HOST_PROD=your_supabase_host_prod_here
DB_PORT_PROD=5432
DB_USER_PROD=postgres
DB_PASSWORD_PROD=your_supabase_password_prod_here
DB_NAME_PROD=postgres

# Supabase Configuration
SUPABASE_URL=your_supabase_url_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# dbt Configuration
DBT_TARGET=dev
DBT_PROFILES_DIR=.