{
  "tailwindCSS": {
    "classRegex": [
      ["clsx\\(([^)]*)\\)", "(?:'|\"|`)([^']*)(?:'|\"|`)"],
      ["classnames\\(([^)]*)\\)", "(?:'|\"|`)([^']*)(?:'|\"|`)"],
      ["cn\\(([^)]*)\\)", "(?:'|\"|`)([^']*)(?:'|\"|`)"],
      ["tw\\`([^`]*)\\`", "([^`]*)"]
    ],
    "experimental": {
      "classRegex": [
        "tw\\`([^`]*)"
      ]
    }
  }
}