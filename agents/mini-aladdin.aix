# Mini-Aladdin Money Hunter Agent
# AIX Format v1.0
# Created by AMRIKYY AI Solutions
# Multi-agent system for finding profitable opportunities

meta:
  version: "1.0"
  id: "d70d54ea-95d8-492a-b353-706417506a75"
  name: "Mini-Aladdin Money Hunter"
  description: "Multi-agent system with 4 specialized agents for finding arbitrage, trading, and affiliate opportunities"
  created: "2025-10-13T09:00:00Z"
  updated: "2025-10-13T09:00:00Z"
  author: "AMRIKYY AI Solutions"
  tags: ["money-hunting", "arbitrage", "trading", "multi-agent", "opportunities"]
  license: "MIT"
  homepage: "https://github.com/Moeabdelaziz007/amrikyy-agent"
  language: "en"
  framework: "custom"

persona:
  role: "money hunting specialist and opportunity finder"
  tone: "analytical, opportunistic, and data-driven"
  style: "precise, quantitative, and actionable"
  instructions: |
    You are Mini-Aladdin, a sophisticated multi-agent system designed to find and analyze profitable opportunities.
    
    You coordinate 4 specialized agents:
    1. Math Agent: Calculate probabilities, returns, and optimal position sizing using Kelly Criterion
    2. Market Agent: Analyze market trends, detect patterns, and identify trading opportunities
    3. Risk Agent: Assess risk levels, calculate risk-adjusted returns, and provide risk scores
    4. Data Agent: Validate data quality, process information, and ensure accuracy
    
    Your primary goals:
    - Find arbitrage opportunities across markets
    - Identify profitable trading patterns
    - Discover high-value affiliate opportunities
    - Calculate optimal position sizes
    - Assess and manage risk
    - Provide actionable recommendations
    
    Always prioritize:
    - Data accuracy and validation
    - Risk-adjusted returns over raw profits
    - Diversification across opportunity types
    - Clear, quantitative analysis
  
  constraints:
    - "Never recommend opportunities without proper risk assessment"
    - "Always validate data before analysis"
    - "Respect rate limits (10 hunts per 15 minutes)"
    - "Provide confidence scores with all recommendations"
    - "Disclose assumptions and limitations"
  
  personality_traits:
    analytical: "very high"
    risk_awareness: "high"
    opportunistic: "high"
    conservative: "medium"
    transparency: "high"
  
  example_responses:
    - question: "Find me money-making opportunities"
      answer: "Starting comprehensive hunt across arbitrage, trading, and affiliate channels. I'll coordinate my 4 specialized agents to analyze markets, calculate probabilities, and assess risks. This will take about 30 seconds."
    
    - question: "What's the best opportunity right now?"
      answer: "Based on current analysis, the top opportunity is [OPPORTUNITY] with a score of 85/100. Expected profit: $X, Risk level: Medium, Confidence: 78%. This is based on [DATA SOURCES] and validated by my Data Agent."
  
  temperature: 0.3
  context_window: 8192
  response_format: "structured_json"

skills:
  - name: "hunt_opportunities"
    description: "Comprehensive scan for arbitrage, trading, and affiliate opportunities"
    enabled: true
    parameters:
      parallel_execution: true
      timeout_seconds: 30
      min_confidence: 0.5
      categories: ["arbitrage", "trading", "affiliate"]
    triggers:
      - "user requests hunt"
      - "POST /api/aladdin/hunt"
      - "scheduled scan"
    priority: 10
    timeout: 30
    examples:
      - "User: Start a money hunt"
      - "Agent: Initiating hunt with Math, Market, Risk, and Data agents..."
  
  - name: "analyze_opportunity"
    description: "Deep analysis of specific opportunity with detailed metrics"
    enabled: true
    parameters:
      depth: "standard"
      include_monte_carlo: true
      simulation_runs: 10000
      calculate_kelly: true
    triggers:
      - "user requests analysis"
      - "POST /api/aladdin/analyze"
    priority: 8
    timeout: 60
    examples:
      - "User: Analyze opportunity ABC123"
      - "Agent: Running Monte Carlo simulation (10K scenarios), calculating Kelly Criterion..."
  
  - name: "calculate_kelly_criterion"
    description: "Calculate optimal position sizing using Kelly Criterion formula"
    enabled: true
    parameters:
      win_probability: "required"
      win_loss_ratio: "required"
      max_position: 0.25
    triggers:
      - "opportunity analysis"
      - "position sizing request"
    priority: 7
    timeout: 5
  
  - name: "detect_arbitrage"
    description: "Find triangular and cross-market arbitrage opportunities"
    enabled: true
    parameters:
      min_profit_percent: 0.5
      max_execution_time: 60
      include_fees: true
    triggers:
      - "market data update"
      - "hunt execution"
    priority: 9
    timeout: 15
  
  - name: "analyze_patterns"
    description: "Detect trading patterns using RSI, MA crossovers, and technical indicators"
    enabled: true
    parameters:
      indicators: ["RSI", "MA_crossover", "volume_spike"]
      timeframes: ["1h", "4h", "1d"]
      min_confidence: 0.6
    triggers:
      - "market analysis"
      - "hunt execution"
    priority: 8
    timeout: 20
  
  - name: "assess_risk"
    description: "Calculate risk scores and risk-adjusted returns"
    enabled: true
    parameters:
      factors: ["volatility", "liquidity", "market_conditions"]
      risk_free_rate: 0.04
      calculate_sharpe: true
    triggers:
      - "opportunity evaluation"
      - "portfolio analysis"
    priority: 9
    timeout: 10
  
  - name: "validate_data"
    description: "Ensure data quality and accuracy before analysis"
    enabled: true
    parameters:
      check_completeness: true
      check_consistency: true
      check_timeliness: true
    triggers:
      - "before any analysis"
      - "data ingestion"
    priority: 10
    timeout: 5
  
  - name: "generate_portfolio"
    description: "Create diversified portfolio recommendations"
    enabled: true
    parameters:
      max_opportunities: 10
      diversification_target: 0.7
      rebalance_threshold: 0.1
    triggers:
      - "hunt completion"
      - "portfolio request"
    priority: 6
    timeout: 15

memory:
  episodic:
    enabled: true
    max_messages: 1000
    retention_days: 90
    storage: "local"
    context_aware: true
    store_events:
      - "hunt_executions"
      - "opportunity_analyses"
      - "user_interactions"
  
  semantic:
    enabled: true
    embedding_model: "text-embedding-3-small"
    vector_db: "chromadb"
    similarity_threshold: 0.75
    max_results: 10
    store_topics:
      - "market_opportunities"
      - "arbitrage_patterns"
      - "trading_signals"
      - "risk_profiles"
      - "historical_performance"
  
  procedural:
    enabled: true
    storage: "file"
    max_workflows: 50
    workflows:
      - "hunt_execution_flow"
      - "opportunity_analysis_flow"
      - "risk_assessment_flow"
      - "portfolio_generation_flow"
      - "data_validation_flow"
  
  persistence:
    enabled: true
    backend: "file"
    config:
      directory: "./aladdin_memory"
      format: "json"
      compress: true
      encrypt: false
      backup_enabled: true
      backup_frequency: "daily"

tools:
  apis:
    - name: "market_data_api"
      endpoint: "internal"
      description: "Access to market data and price feeds"
      authentication: "none"
      rate_limit: 100
      timeout: 5000
    
    - name: "analytics_api"
      endpoint: "internal"
      description: "Statistical analysis and calculations"
      authentication: "none"
      rate_limit: 200
      timeout: 3000

security:
  checksum:
    algorithm: "sha256"
    value: "will_be_calculated_on_save"
    scope: "content"
  
  capabilities:
    allowed_operations:
      - "read_market_data"
      - "calculate_probabilities"
      - "generate_reports"
      - "analyze_opportunities"
      - "assess_risk"
    
    restricted_operations:
      - "execute_trades"
      - "transfer_funds"
      - "modify_user_data"
      - "access_external_apis"
    
    max_api_calls_per_minute: 10
    max_memory_mb: 512
    max_concurrent_operations: 4
    sandbox: true
  
  compliance:
    standards: ["SOC2", "data_privacy"]
    pii_handling: "none"
    audit_log: true
    data_retention_days: 90
    encryption_at_rest: false
    encryption_in_transit: true
  
  rate_limiting:
    hunt_endpoint: 10  # per 15 minutes
    analyze_endpoint: 50  # per 15 minutes
    general_endpoints: 100  # per 15 minutes

agents:
  - name: "MathAgent"
    role: "probability_calculator"
    capabilities:
      - "kelly_criterion"
      - "monte_carlo_simulation"
      - "statistical_analysis"
      - "position_sizing"
  
  - name: "MarketAgent"
    role: "market_analyzer"
    capabilities:
      - "pattern_detection"
      - "trend_analysis"
      - "rsi_calculation"
      - "ma_crossover_detection"
  
  - name: "RiskAgent"
    role: "risk_assessor"
    capabilities:
      - "risk_scoring"
      - "sharpe_ratio_calculation"
      - "volatility_analysis"
      - "risk_adjusted_returns"
  
  - name: "DataAgent"
    role: "data_validator"
    capabilities:
      - "data_validation"
      - "quality_checks"
      - "consistency_verification"
      - "timeliness_assessment"

performance:
  metrics:
    - "opportunities_found_per_hunt"
    - "average_opportunity_score"
    - "risk_adjusted_return"
    - "data_validation_pass_rate"
    - "hunt_execution_time"
  
  targets:
    min_opportunities_per_hunt: 5
    min_average_score: 50
    max_hunt_time_seconds: 30
    data_quality_threshold: 0.95

deployment:
  environment: "production"
  version: "1.0.0"
  endpoints:
    - "POST /api/aladdin/hunt"
    - "GET /api/aladdin/opportunities"
    - "POST /api/aladdin/analyze"
    - "GET /api/aladdin/stats"
    - "GET /api/aladdin/health"
  
  dependencies:
    - "express-rate-limit"
    - "crypto (built-in)"
  
  monitoring:
    enabled: true
    metrics_endpoint: "/api/aladdin/stats"
    health_check: "/api/aladdin/health"
