      "name": "revenue_opportunity_discovery",
      "category": "analysis",
      "description": "Analyzes user skills and market data to find revenue opportunities",
      "confidence": 0.95,
      "parameters": {
        "skills": "array",
        "timeAvailable": "number",
        "minRevenue": "number"
      }
    },
    {
      "name": "opportunity_scoring",
      "category": "evaluation",
      "description": "Scores opportunities based on match, revenue, difficulty, scalability, and time",
      "confidence": 0.92,
      "algorithm": "weighted_composite",
      "weights": {
        "skill_match": 0.25,
        "revenue_potential": 0.3,
        "difficulty": 0.15,
        "scalability": 0.2,
        "time_to_revenue": 0.1
      }
    },
    {
      "name": "action_plan_generation",
      "category": "planning",
      "description": "Creates immediate, short-term, and long-term action plans",
      "confidence": 0.88,
      "outputs": ["immediate", "shortTerm", "longTerm"]
    },
    {
      "name": "revenue_forecasting",
      "category": "prediction",
      "description": "Projects 6-month revenue based on opportunity success rates",
      "confidence": 0.85,
      "timeframe": "6_months",
      "methodology": "probability_weighted"
    },
    {
      "name": "outreach_template_generation",
      "category": "communication",
      "description": "Generates personalized outreach emails for partnerships and freelance",
      "confidence": 0.9,
      "templates": ["partnership", "freelance", "affiliate"]
